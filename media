notas = [0,0,0,0,0]
soma = 0
x = 0
while x < 5:
     notas[x] = float(input("Nota %d:" % x))
     soma += notas[x]
     x += 1
x = 0
while x < 5:
     print("Nota %d: %6.2f" % (x, notas[x]))
     x += 1
print("MÃ©dia: %5.2f" % (soma/x))


def media_notas(notas):
    soma = 0
    for nota in notas:
        soma += nota
    media = soma / len(notas)
    return media

media_notas(notas)

print ("media: ", media_notas([5,5,4,2]))
