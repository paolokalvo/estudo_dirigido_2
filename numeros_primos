num = int( input("Digite um numero :"))
tot = 0
for c in range(1, num + 1):
    if num % c == 0 :
        print('\033[20m', end=' ')
        tot += 1
    else:
        print('\033[10m',end=' ')
    print('{}.'.format(c),end=' ')
print('\n\033[m0 numero {}  foi divisivel {} vezes '.format(num,tot))
if tot == 2:
    print('\033[34m''É Primo!!! ')
else:
    print('Não é Primo !!!!')
